  $(".disabled-image").click(function(){
  
      $('.btn-addtocart').prop('disabled', true);
    
    
    
      $(".btn-addtocart span:nth-of-type(2)").html("Sold out");
  
  });
      
      
                        
  $(".normal_button").click(function(){
  
      $('.btn-addtocart').prop('disabled', false);
    
    
    //$("p:nth-of-type(3)").css("background-color", "yellow");
    $(".btn-addtocart span:nth-of-type(2)").html("Add to Cart");
  
  });
    
});
    
    
</script>


